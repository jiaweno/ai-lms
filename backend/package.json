{
  "name": "ai-lms-backend",
  "version": "1.0.0",
  "description": "AI Learning Management System Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "workers:dev": "tsx watch src/workers/index.ts"
  },
  "keywords": [
    "ai",
    "learning",
    "management",
    "system",
    "fastify",
    "typescript"
  ],
  "author": "AI LMS Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "@fastify/helmet": "^10.1.0",
    "@fastify/jwt": "^6.7.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.0",
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.11.5",
    "compromise": "^14.10.0",
    "fastify": "^4.17.0",
    "ioredis": "^5.3.1",
    "joi": "^17.9.1",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.0",
    "node-nlp": "^4.27.0",
    "openai": "^3.2.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.5",
    "@swc/core": "^1.3.50",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/jest": "^29.5.0",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/pdf-parse": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "winston": "^3.8.2"
  }
}
